        <description>This function will validate the requested for mobile number</description>
        <mobile_callable>false</mobile_callable>
        <name>validateRequestForMobileNumber</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[function validateRequestForMobileNumber(phoneNumber) {
	var isValid = false;
    const regex = /^\+?([0-9]{1,3})?[-. ]?(\(?[0-9]{1,4}\)?)?[-. ]?([0-9]{1,4})[-. ]?([0-9]{1,4})[-. ]?([0-9]{1,9})$/;
	if (regex.test(phoneNumber) == true)
	{
		isValid = true;
	}

    return isValid;
}]]></script>
